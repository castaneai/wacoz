{
  "name": "wacoz",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "ニコニコ生放送のアニメ放送を予約",
  "icons": {
    "16": "images/icon-16.png"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png"
    },
    "default_title": "wacoz",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "http://live.nicovideo.jp/*",
    "http://nl.simg.jp/*"
  ]
}